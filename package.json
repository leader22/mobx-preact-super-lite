{
  "name": "mobx-preact-super-lite",
  "version": "2.0.7",
  "description": "Super lightweight Preact bindings for MobX based on Preact 10.x+ and Hooks",
  "main": "dist/index.js",
  "jsnext:main": "dist/mobxpreactsuperlite.esm.js",
  "module": "dist/mobxpreactsuperlite.esm.js",
  "unpkg": "dist/mobxpreactsuperlite.umd.production.min.js",
  "jsdelivr": "dist/mobxpreactsuperlite.umd.production.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "npm run lint:es && npm run lint:ts",
    "lint:es": "eslint . --ext .ts,.tsx",
    "lint:ts": "tsc --noEmit",
    "test": "jest",
    "build": "tsdx build --name mobxPreactSuperLite --format=cjs,esm,umd --tsconfig ./tsconfig.build.json"
  },
  "keywords": [
    "mobx",
    "preact",
    "hooks"
  ],
  "author": "Yuji Sugiura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leader22/mobx-preact-super-lite.git"
  },
  "bugs": {
    "url": "https://github.com/leader22/mobx-preact-super-lite/issues"
  },
  "homepage": "https://github.com/leader22/mobx-preact-super-lite",
  "peerDependencies": {
    "mobx": "^6.0.0",
    "preact": "^10.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/preact": "^2.0.0",
    "@testing-library/preact-hooks": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.3.0",
    "jest-mock-console": "^1.0.1",
    "mobx": "^6.0.0",
    "preact": "^10.5.3",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "tsdx": "^0.13.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {}
}
